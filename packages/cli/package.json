{
  "name": "@agenthive/cli",
  "version": "1.0.0",
  "description": "AgentHive Command Line Interface - Control your AI agent swarm from the terminal",
  "type": "module",
  "bin": {
    "hive": "./bin/hive",
    "memory": "./bin/memory"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts --max-warnings 0",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "graphql": "^16.7.1",
    "graphql-request": "^6.1.0",
    "@memory-manager/shared": "*",
    "chalk": "^5.3.0",
    "inquirer": "^9.2.8",
    "ora": "^7.0.1",
    "conf": "^11.0.2",
    "keytar": "^7.9.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "tsx": "^3.12.7",
    "typescript": "^5.0.2"
  }
}